{
  "overrides": [
    {
      "files": "./**/*.html",
      "options": {
        "printWidth": 80
      }
    },
    {
      "files": "./**/*.ejs",
      "options": {
        "printWidth": 500
      }
    },
    {
      "files": ["./**/*.js"],
      "options": {
        "trailingComma": "es5",
        "printWidth": 160
      }
    }
  ]
}
